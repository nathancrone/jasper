@model IEnumerable<Jasper.Models.NotificationApp>

@{
    ViewBag.Title = "Index";
}
<div class="row">

    <h2>Index</h2>

    <p>
        @Html.ActionLink("Create New", "AppCreate")
    </p>

    <table>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AppName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HubName)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.AppName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.HubName)
                </td>
                <td>
                    @Html.ActionLink("Edit", "AppEdit", new { id = item.AppId }) |
                    @Html.ActionLink("Details", "AppDetails", new { id = item.AppId }) |
                    @Html.ActionLink("Tags", "TagIndex", new { id = item.AppId }) |
                    @Html.ActionLink("Delete", "AppDelete", new { id = item.AppId })
                </td>
            </tr>
        }

    </table>


</div>